{
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["storage"], 
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "providerScript.bundle.js"
      ]
    }
  ]

  // TODO: Maybe add web accessible resources if needed here? 
}
