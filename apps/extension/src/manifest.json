{
  "manifest_version": 3,
  "name": "UL Wallet Extension",
  "description": "UL Wallet Extension",
  "version": "0.0.0.0",
  "icons": {
    "64": "assets/default.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["storage", "notifications"], 
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "providerScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "injected.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
